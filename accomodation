#!/usr/bin/python

def factorial(number):
    factorial = 1
    iterator = int(number);

    if (number == 0):
        return 1

    for i in range(1,  number):
        factorial *= number


    return factorial


def accomodation(elemets, by):
    result =  factorial(elemets) /  factorial(elements - by)
    print(result);

print("Input nuber of elements");
elements = int(input())
print("Input by numbers to  accomodation");
by = int(input())

accomodation(elements, by)

